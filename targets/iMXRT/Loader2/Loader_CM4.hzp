<!DOCTYPE CrossStudio_Project_File>
<solution Name="iMXRT Loader CM4" target="8" version="2">
  <configuration
    Name="Common"
    Platform="ARM"
    arm_instruction_set="Thumb"
    arm_library_instruction_set="Thumb"
    c_additional_options="-fno-signed-char;-Wno-narrowing;-Wall;-Wextra;-Wmisleading-indentation;-Wshadow;-Wdouble-promotion;-Wformat=2"
    c_preprocessor_definitions="__THUMB;STARTUP_FROM_RESET=1;DISABLE_WDOG=1"
    c_preprocessor_undefinitions="__STDCPP_THREADS__"
    clang_tidy_checks_cpp="*;-llvm*;-fuchsia*;-modernize-use-trailing-return-type;-*braces-around-statements;-*magic-numbers;-cppcoreguidelines-pro-bounds-constant-array-index;-google-readability-namespace-comments"
    cpp_only_additional_options="-Woverloaded-virtual;-Wnon-virtual-dtor" />
  <configuration
    Name="Debug"
    c_preprocessor_definitions="DEBUG=1"
    gcc_debugging_level="Level 3"
    gcc_omit_frame_pointer="Yes"
    gcc_optimization_level="None"
    hidden="Yes" />
  <configuration
    Name="Release"
    c_preprocessor_definitions="NDEBUG=1"
    gcc_debugging_level="None"
    gcc_omit_frame_pointer="Yes"
    gcc_optimization_level="Level 1"
    hidden="Yes" />
  <configuration
    Name="1170"
    Target="MIMXRT1176DVMAA_cm4"
    c_preprocessor_definitions="CPU_MIMXRT1176DVMAA_cm4"
    c_user_include_directories=".;./iMXRT1170/."
    hidden="Yes"
    linker_memory_map_file="../MemoryMap_MIMXRT117x_cm4.xml"
    macros="DeviceName=MIMXRT1176_cm4" />
  <configuration
    Name="MIMXRT1170_Debug"
    build_output_file_name="Loader_iMXRT1170_M4_Debug.elf"
    inherited_configurations="1170;Debug" />
  <configuration
    Name="MIMXRT1170_Release"
    build_output_file_name="Loader_iMXRT1170_M4.elf"
    inherited_configurations="1170;Release" />
  <project Name="Loader_CM4">
    <configuration
      CMSIS_CORE="Yes"
      Name="Common"
      Placement="TCM"
      arm_architecture="v7EM"
      arm_core_type="Cortex-M4"
      arm_fp_abi="Hard"
      arm_fpu_type="FPv4-SP-D16"
      arm_library_optimization="Small"
      arm_linker_heap_size="1024"
      arm_linker_stack_size="2048"
      arm_simulator_memory_simulation_parameter="ROM;0x08000000;0x04000000;RAM;0x1FFE0000;0x00010000;RAM;0x20000000;0x00010000;RAM;0x20240000;0x00080000;ROM;0x30000000;0x04000000"
      arm_target_loader_applicable_loaders=""
      arm_target_loader_default_loader=""
      batch_build_configurations="MIMXRT1170_Release"
      c_system_include_directories="$(StudioDir)/include;$(PackagesDir)/include;$(PackagesDir)/libraries/libmem_drivers"
      c_user_include_directories=".;./.."
      debug_register_definition_file="../XML/$(DeviceName)_registers.xml"
      debug_startup_completion_point="main"
      debug_type_file="$(StudioDir)/bin/libc++.xml"
      gcc_c_language_standard="c11"
      gcc_cplusplus_language_standard="c++14"
      libcxx="Yes (No Exceptions)"
      linker_additional_files="$(StudioDir)/lib/libmem$(LibExt)$(LIB);$(StudioDir)/lib/libmem_loader$(LibExt)$(LIB)"
      linker_keep_symbols="libmem_crc32"
      linker_section_placement_file="../placement_tcm.xml"
      project_directory=""
      project_type="Executable"
      property_groups_file_path="$(ProjectDir)/../propertyGroups_cm4.xml"
      target_connect_script="Connect()"
      target_get_partname_script="GetPartName()"
      target_reset_script="Reset_Loader()"
      target_script_file="$(ProjectDir)/../iMXRT_Target.js" />
    <folder Name="System Files">
      <file file_name="thumb_crt0.s" />
      <file file_name="../system_$(DeviceName).h" />
      <file file_name="../system_$(DeviceName).c" />
      <file file_name="../$(DeviceName).h" />
      <file file_name="../$(DeviceName)_features.h" />
      <file file_name="../fsl_device_registers.h" />
      <file file_name="startup.s" />
      <file file_name="$(ProjectDir)/../iMXRT_Target.js">
        <configuration Name="Common" file_type="Target Script" />
      </file>
      <configuration Name="Common" gcc_c_language_standard="gnu11" />
    </folder>
    <folder Name="drivers">
      <file file_name="fsl_common.c" />
      <file file_name="fsl_common.h" />
      <file file_name="fsl_flexspi.c" />
      <file file_name="fsl_flexspi.h" />
      <file file_name="fsl_dmamux.c" />
      <file file_name="fsl_dmamux.h" />
      <file file_name="pin_mux.h" />
      <file file_name="fsl_lpuart.c" />
      <file file_name="fsl_lpuart.h" />
      <folder Name="iMXRT1170">
        <configuration Name="Common" build_exclude_from_build="Yes" />
        <configuration Name="1170" build_exclude_from_build="No" />
        <file file_name="iMXRT1170/fsl_clock.c" />
        <file file_name="iMXRT1170/fsl_clock.h" />
        <file file_name="iMXRT1170/fsl_iomuxc.h" />
        <file file_name="iMXRT1170/clock_config.h" />
        <file file_name="iMXRT1170/clock_config.c" />
        <file file_name="iMXRT1170/fsl_anatop_ai.h" />
        <file file_name="iMXRT1170/fsl_anatop_ai.c" />
        <file file_name="iMXRT1170/pin_mux.c" />
        <file file_name="iMXRT1170/board.h" />
        <file file_name="iMXRT1170/fsl_gpio.c" />
        <file file_name="iMXRT1170/fsl_gpio.h" />
        <file file_name="iMXRT1170/fsl_pmu.c" />
        <file file_name="iMXRT1170/fsl_pmu.h" />
        <file file_name="iMXRT1170/cm4/fsl_cache.c" />
        <file file_name="iMXRT1170/cm4/fsl_cache.h" />
        <file file_name="../MemoryMap_MIMXRT117x_cm4.xml" />
      </folder>
    </folder>
    <folder Name="Source">
      <file file_name="loader.cpp" />
      <file file_name="board.cpp" />
      <file file_name="libmem_driver_Hyperflash.h" />
      <file file_name="libmem_driver_Hyperflash.cpp" />
      <file file_name="libmem_Tools.h" />
      <file file_name="libmem_Tools.cpp" />
      <file file_name="libmem_driver_xSPI.h" />
      <file file_name="libmem_driver_xSPI.cpp" />
      <file file_name="DebugPrint.h" />
      <file file_name="DebugPrint.cpp" />
      <file file_name="DebugPrint_USART.cpp" />
      <file file_name="board_generic.h" />
      <file file_name="libmem_LUT_Generic.h" />
      <file file_name="libmem_LUT_Adesto.h" />
      <file file_name="libmem_LUT_Adesto.cpp" />
      <file file_name="libmem_LUT_Macronix.h" />
      <file file_name="libmem_LUT_Macronix.cpp" />
    </folder>
    <folder Name="libmem">
      <file file_name="$(StudioDir)/include/libmem.h" />
      <file file_name="$(StudioDir)/include/libmem_loader.h" />
    </folder>
  </project>
</solution>
