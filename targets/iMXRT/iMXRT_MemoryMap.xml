<!DOCTYPE Board_Memory_Definition_File>
<Root name="iMXRT" >
  <MemorySegment name="NCACHE_REGION" start="$(NCACHE_REGION_START:0x20210000)" size="$(NCACHE_REGION_SIZE:0x0)"/>
  <MemorySegment name="SDRAM"  start="0x80000000" size="$(RAMSIZE:0x1e00000)"/>
  <MemorySegment name="FlexSPI2" start="$(FlexSPI2START:0x70000000)" size="$(FlexSPI2SIZE:0x04000000)" access="ReadOnly"/>
  <MemorySegment name="FlexSPI" start="$(FlexSPISTART:0x60000000)" size="$(FlexSPISIZE:0x04000000)" access="ReadOnly"/>
  <MemorySegment name="OCRAM" start="$(OCRAMSTART:0x20200000)" size="$(OCRAMSIZE:0x00040000)"/>
  <MemorySegment name="DTCM" start="$(DTCMSTART:0x20000000)" size="$(DTCMSIZE:0x00020000)"/>
  <MemorySegment name="ITCM" start="$(ITCMSTART:0x00000000)" size="$(ITCMSIZE:0x00020000)"/>
</Root>
