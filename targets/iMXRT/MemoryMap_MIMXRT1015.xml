<!DOCTYPE Board_Memory_Definition_File>
<Root name="iMXRT" >
	<MemorySegment name="FlexSPI"     start="$(FlexSPISTART:0x60000000)" size="$(FlexSPISIZE:0x04000000)" access="ReadOnly"/>
	<MemorySegment name="OCRAM"       start="$(OCRAMSTART:0x20200000)"   size="$(OCRAMSIZE:0x00010000)"/>
	<MemorySegment name="DTCM"        start="$(DTCMSTART:0x20000000)"    size="$(DTCMSIZE:0x00008000)"/>
	<MemorySegment name="ITCM"        start="$(ITCMSTART:0x00000000)"    size="$(ITCMSIZE:0x00008000)"/>
</Root>
