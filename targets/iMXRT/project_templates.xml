<projects>
	<project cpu_manufacturer="NXP" cpu_name="iMXRT_CM7" name="iMXRT Executable" description="An executable for a generic iMXRT-CM7 processor." type="Executable" template_name="iMXRT_EXE">
		<configuration name="THUMB Debug" />
		<configuration name="THUMB Release" />
		<property name="property_groups_file_path" value="$(TargetsDir)/iMXRT/propertyGroups.xml" />
		<propertygroup name="Target" value="MIMXRT1052DVL6B" choose="Yes" />
		<propertygroup name="Placement" value="TCM" choose="Yes" />
		<property name="arm_architecture" value="v7EM" />
		<property name="arm_core_type" value="Cortex-M7" />
		<property name="arm_fp_abi" value="Hard" />
		<property name="arm_simulator_memory_simulation_parameter" value="RAM;0x00000000;0x00020000;RAM;0x20000000;0x00020000;RAM;0x20200000;0x00040000;ROM;0x60000000;0x04000000" />
		<property name="debug_startup_completion_point" value="main" />
		<property name="target_connect_script" value="Connect()" />
		<property name="target_get_partname_script" value="GetPartName()" />
		<property name="target_reset_script" value="Reset()" />
		<property name="target_script_file" value="$(TargetsDir)/iMXRT/iMXRT_Target.js" />
		<property name="arm_target_loader_reset_after_download" value="No" />
		<property name="c_system_include_directories" value="$(StudioDir)/include;$(PackagesDir)/include;$(TargetsDir)/iMXRT" />
		<property name="arm_linker_stack_size" value="1024" />
		<property name="arm_linker_heap_size" value="1024" />
		<property name="linker_output_format" />
		<property name="linker_printf_fmt_level" />
		<property name="linker_printf_width_precision_supported" />
		<property name="linker_printf_fp_enabled" />
		<property name="linker_scanf_fmt_level" />
		<property name="linker_scanf_fp_enabled" />
		<property name="linker_scanf_character_group_matching_enabled" />
		<property name="debug_register_definition_file" value="$(TargetsDir)/iMXRT/XML/$(DeviceName)_registers.xml"/>
		<property name="arm_target_flash_loader_file_path" value="$(TargetsDir)/iMXRT/Loader2/Loader_$(Family).elf" configuration="Flash" />
		<propertygroup name="CMSIS_CORE" value="Yes" />
		<folder name="Source Files" filter="c;cpp;cxx;cc;h;s;asm;inc">
			<file name="main.c" source="$(StudioDir)/samples/Shared/main.c" />
		</folder>
		<folder name="System Files">
			<file name="$(StudioDir)/source/thumb_crt0.s" expand="no" />
			<file name="$(TargetsDir)/iMXRT/startup_$(DeviceName).s" expand="no" />
			<file name="$(TargetsDir)/iMXRT/system_$(DeviceName).h" expand="no" />
			<file name="$(TargetsDir)/iMXRT/system_$(DeviceName).c" expand="no" />
			<file name="$(TargetsDir)/iMXRT/$(DeviceName).h" expand="no" />
			<file name="$(TargetsDir)/iMXRT/$(DeviceName)_features.h" expand="no" />
			<file name="$(TargetsDir)/iMXRT/fsl_device_registers.h" expand="no" />
		</folder>
	</project>

