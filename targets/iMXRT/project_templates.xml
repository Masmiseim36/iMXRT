    <property name="c_system_include_directories" value="$(ARMGCCDIR)/../arm-none-eabi/include;$(ARMGCCDIR)/../lib/gcc/arm-none-eabi/$(ARMGCCVERSION)/include;$(ARMGCCDIR)/../lib/gcc/arm-none-eabi/$(ARMGCCVERSION)/include-fixed;$(ARMGCCDIR)/../arm-none-eabi/include/c++/$(ARMGCCVERSION);$(ARMGCCDIR)/../arm-none-eabi/include/c++/$(ARMGCCVERSION)/bits" />
    <property name="use_external_gcc" value="Yes" />
    <property name="build_toolchain_directory" value="$(ARMGCCDIR)" />
    <property name="gcc_prefix" value="$(ARMGCCPREFIX)" />
  </project>
  <project cpu_manufacturer="NXP" cpu_name="iMXRT" name="iMXRT Externally Built Executable" description="An externally built executable for NXP iMXRT processor." type="Externally Built Executable" template_name="iMXRT_EXT_EXE">
    <property name="property_groups_file_path" value="$(TargetsDir)/iMXRT/propertyGroups.xml" />
    <propertygroup name="Target" value="MIMXRT1052DVL6B" choose="Yes" />
