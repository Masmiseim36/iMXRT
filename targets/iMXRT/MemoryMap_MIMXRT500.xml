<!DOCTYPE Board_Memory_Definition_File>
<Root name="iMXRT" >
	<MemorySegment name="FlexSPI0"      start="0x08000000" size="$(FlexSPISIZE:0x08000000)" access="ReadOnly"/>
	<MemorySegment name="FlexSPI1"      start="0x28000000" size="$(FlexSPISIZE:0x08000000)" access="ReadOnly"/>
	<MemorySegment name="SRAM_code"     start="0x00000000" size="$(SRAMSIZE:0x00500000)"    access="Read/Write"/>
	<MemorySegment name="SRAM_code_sec" start="0x10000000" size="$(SRAMSIZE:0x00500000)"    access="Read/Write"/>
	<MemorySegment name="SRAM_data"     start="0x20000000" size="$(SRAMSIZE:0x00500000)"    access="Read/Write"/>
	<MemorySegment name="SRAM_data_sec" start="0x30000000" size="$(SRAMSIZE:0x00500000)"    access="Read/Write"/>
</Root>
